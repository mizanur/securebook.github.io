{
	"compilerOptions": {
		"target": "esnext",
		"resolveJsonModule": true,
		"experimentalDecorators": true,
		"moduleResolution": "node",
		"allowJs": true,
		"noEmit": true,
		"strict": true,
		"isolatedModules": true,
		"esModuleInterop": true,
		"noImplicitAny": false,
		"skipLibCheck": true,
		"jsx": "react",
		"jsxFactory": "h",
		"strictPropertyInitialization": false,
		"types": [],
		"baseUrl": "./src",
		"paths": {
			"@interfaces/*": ["interfaces/*"]
		}
	},
	"include": [
		"src"
	]
}